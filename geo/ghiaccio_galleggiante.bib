	file = {Łokas et al. - 2022 - Unveiling the extreme environmental radioactivity .pdf:/home/ag/Zotero/storage/JE32R6SF/Łokas et al. - 2022 - Unveiling the extreme environmental radioactivity .pdf:application/pdf},
}

@article{barendregt_challenge_2005,
