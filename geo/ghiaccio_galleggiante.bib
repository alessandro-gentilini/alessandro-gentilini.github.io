	file = {Strebelle - 2021 - Multiple-Point Statistics Simulation Models Prett.pdf:/home/ag/Zotero/storage/2JSRHJN3/Strebelle - 2021 - Multiple-Point Statistics Simulation Models Prett.pdf:application/pdf},
}

