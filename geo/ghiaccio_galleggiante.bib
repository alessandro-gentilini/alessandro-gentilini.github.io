	pages = {20201521},
	file = {How et al. - 2020 - Zebra stripes, tabanid biting flies and the aperture effect.pdf:/home/ag/Zotero/storage/ZAYGKPK4/How et al. - 2020 - Zebra stripes, tabanid biting flies and the aperture effect.pdf:application/pdf},
}

@article{caplan-auerbach_every_2009,
