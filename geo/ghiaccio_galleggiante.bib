	pages = {12--20},
	file = {Staub et al. - 2002 - Effectiveness of a Repellent Containing DEET and EBAAP for Preventing Tick Bites.pdf:/home/ag/Zotero/storage/7THXSWFY/Staub et al. - 2002 - Effectiveness of a Repellent Containing DEET and EBAAP for Preventing Tick Bites.pdf:application/pdf},
