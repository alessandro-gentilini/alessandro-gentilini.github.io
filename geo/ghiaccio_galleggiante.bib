@article{erosheva_breaking_2014,
	title = {Breaking {Bad}: {Two} {Decades} of {Life}-{Course} {Data} {Analysis} in {Criminology}, {Developmental} {Psychology}, and {Beyond}},
	volume = {1},
	issn = {2326-8298, 2326-831X},
	shorttitle = {Breaking {Bad}},
