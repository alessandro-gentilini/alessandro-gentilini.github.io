	doi = {10.20944/preprints202005.0184.v1},
	file = {Badii et al. - 2020 - Impact on Mobility and Environmental Data of COVID-19 Lockdown on Florence Area.pdf:/home/ag/Zotero/storage/Z5VMG4YA/Badii et al. - 2020 - Impact on Mobility and Environmental Data of COVID-19 Lockdown on Florence Area.pdf:application/pdf},
