	file = {Dal Moro - 2020 - On the Identification of Industrial Components in the Horizontal-to-Vertical Spectral Ratio (HVSR) from Microtremors.pdf:/home/ag/Zotero/storage/AG2JNZMA/Dal Moro - 2020 - On the Identification of Industrial Components in the Horizontal-to-Vertical Spectral Ratio (HVSR) from Microtremors.pdf:application/pdf},
