@techreport{muller_renewable_2023,
	type = {preprint},
	title = {Renewable {Energy} in {Warfare}: {Archimedes}’ {Burning} {Mirror}({S}) {Revisited}},
	shorttitle = {Renewable {Energy} in {Warfare}},
	url = {https://www.ssrn.com/abstract=4378110},
	abstract = {The use of renewable energy in warfare has not attracted much attention, but there is one famous exception: during the siege of Syracuse in 212 BC, the Greek mathematician Archimedes is said to have designed a mirror or group of mirrors which used the light of the sun to set fire to Roman ships. The historic sources cannot confirm whether this actually happened. The technical question whether it could have happened, considering the knowledge and technology then available, is still under discussion. The current thinking is that a row of mirrors similar to a Concentrated Solar Power installation is capable of focusing sufficient light to ignite wood, but also that this would be a very impractical weapon. In this article, it is suggested that the combination of two parabolic mirrors, one large reflector and a small collimator, with a plane mirror as directional device can provide, and direct, sufficient energy to ignite timber. The theoretical knowledge and the technological capabilities available at the time indicate that such a renewable energy weapon could have been possible. The principle may have applications for renewable power production today.},
	language = {en},
	urldate = {2024-01-14},
