@article{hudleston_structures_2015,
	title = {Structures and fabrics in glacial ice: {A} review},
	volume = {81},
	issn = {01918141},
	shorttitle = {Structures and fabrics in glacial ice},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0191814115300365},
	doi = {10.1016/j.jsg.2015.09.003},
	abstract = {Glaciers, ice sheets and ice caps represent tectonic systems driven by gravity. Their movement can be studied in real time and the rheological properties and strength of ice determined from laboratory experiments and ﬁeld measurements. All glacial ice has primary stratiﬁcation, exhibited by variations in grain size, bubble content and debris content. As it deforms, with deformation dominated by plastic ﬂow and recrystallization, accompanied locally by fracture under tension, a suite of structures develops that reﬂects the primary fabric of the ice and the anisotropy that develops as a result of cumulative deformation. Initial variations in solid impurity content and strain dependent anisotropy as a result of a crystallographic fabric give rise to effective viscosity increases or decreases compared to isotropic polycrystalline ice of about a factor of ten. Foliation develops from inherited (mostly stratiﬁcation) or introduced (mostly ice veins or fracture traces) fabric elements and from dynamic recrystallization. It is largely dependent on the accumulated strain, which is highest at the base and near the margins of glaciers, ice sheets and ice streams. Folds develop largely passively due to initial ampliﬁcation of irregularities in the primary stratiﬁcation, to variations in ﬂow with time or to inhomogeneous ﬂow associated with shear zones and ductile accommodation around open fractures. Buckle folds and boudinage, mostly on a small scale, occur where viscosity contrast is large, mostly in basal ice. Thrusting and wrench faulting are documented in surging glaciers but theoretically most unlikely and rare or absent elsewhere. Many structures interpreted as faults are not due to shear failure but rather result from shear displacements during opening and closing of tensile fractures.},
	language = {en},
	urldate = {2025-01-06},
	journal = {Journal of Structural Geology},
	author = {Hudleston, Peter J.},
	month = dec,
	year = {2015},
	pages = {1--27},
	file = {PDF:/home/ag/Zotero/storage/N9ST9IEW/Hudleston - 2015 - Structures and fabrics in glacial ice A review.pdf:application/pdf},
}

@book{vai_continuita_2020-1,
	title = {Continuità della {Scuola} {Geologica} {Bolognese} nel {Novecento}},
	isbn = {978-88-6923-623-5},
	url = {https://buponline.com/prodotto/continuita-della-scuola-geologica-bolognese-nel-novecento/?v=c860970b6ab6},
	abstract = {L’Accademia delle Scienze, con il patrocinio dell’Alma Mater Studiorum - Università di Bologna, promuove una serie di incontri volta ad analizzare il percorso scientifico compiuto dalle diverse aree di ricerca dislocate entro il perimetro dell’Alma Mater, nel periodo che corre dai primi anni Cinquanta agli esordi del XXI secolo. L’intento dell’iniziativa non è quello dell’autocelebrazione di una fase che è stata indubbiamente molto positiva nella vita plurisecolare del nostro ateneo, ma di porre in evidenza gli esiti che ha conseguito la sua ricerca sul piano nazionale e internazionale; le reti scientifiche che intorno a queste attività sono nate e si sono sviluppate; l’attività formativa che ne è seguita; i rapporti con le imprese, le amministrazioni pubbliche e altri comparti della vita civile cittadina e regionale. Questa ricognizione, che non è solo di memoria, si propone anche l’obiettivo di segnalare i possibili livelli di crescita e di intersezione tra le frontiere della scienza e della tecnologia e le esigenze della vita poiché la ricerca e la formazione sono chiamate a svolgere compiti molto rilevanti nella società della conoscenza.},
	language = {it},
	urldate = {2025-01-07},
	publisher = {Fondazione Bologna University Press},
	author = {Vai, Gian Battista},
	year = {2020},
	doi = {10.30682/9788869238734},
	annote = {“Il grande geologo canadese Paul F. Hoffman raccontava nel 2004 di aver assistito a una conferenza di Emiliani a Miami nel 1967 su una singola specie di foraminifero il cui guscio si avvolge a sinistra quando è freddo e a destra quando è caldo – proprio come la politica americana – notava ironico (si trattava quasi certamente di Neogloboquadrina pachiderma). Ma ciò che lo colpì maggiormente fu l’oratore italiano “ad alto voltaggio e veloce eloquio”, la cui presentazione era così convinta e convincente da spingerlo per anni a pronunciare i nomi linneani delle specie con lo stesso enfatico accento bolognese in suo onore.” (Vai, 2020, p. 73)
Sicurezza mal riposta?: “Viene così dimostrato che i fattori principali di controllo delle variazioni climatiche sono i parametri astronomici della Terra (eccentricità dell’orbita, obliquità e precessione dell’asse), che regolano le fluttuazioni della curva dell’insolazione.” (Vai, 2020, p. 75)
Critica al termine antropocene: “Si noti anche l’assoluta ortodossia stratigrafica del geologo Emiliani, a paragone delle fantasie eretiche del cosiddetto Antropocene già serpeggianti fra biologi e chimici, in quel caso poco avvezzi al rigore procedurale e alla stabilità degli standard scientifici.” (Vai, 2020, p. 78)
