@article{viaggi_trasgressione_nodate,
	title = {La trasgressione delle "sabbie gialle" nelle prime colline imolesi},
	abstract = {The transgressive "sabbie gialle" in the Imola hills] A new detailed mapping in area of the classic exposures of the "sabbie gialle" formation in the hills South of Imola has definitely shown the unconformable and transgressive attitude of the sands, representing shoreface to foreshore environment, on top of the slightlyfolded Argille Azzurre Formation of early Pleistocene age. Erosional and scattered paleosol evidences beneath the sands are consistent with an emersion interval following the already recognized tectonic phase. The lower part of the "sabbie gialle" formation is locally characterized by a grave1 sandy layer, few cm to 15 m thick, withprominent cherty composition of the well sorted and rounded clasts. It is suggested to limit the use of the term "sabbie gialle" formation to the littoral foreshore to shoreface sandy facies (plus minor heteropic bodies) overlying the unconformity. The underlying regressive, shallow marine, alternating yellow sand and grey siltyclay could be separated as a member within the upper part of the Argille Azzurre Formation.},
	language = {it},
	author = {Viaggi, Paolo},
	pages = {26},
	file = {Viaggi - La trasgressione delle sabbie gialle nelle prime colline imolesi.pdf:/home/ag/Zotero/storage/7V5MGW4A/Viaggi - La trasgressione delle sabbie gialle nelle prime colline imolesi.pdf:application/pdf},
